{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "spellcheck": "cspell docs/**/*.md docs/*.md ../README.md",
    "linkcheck": "markdown-link-check -c md-link-check.json docs/**/*.md docs/*.md ../README.md",
    "checks": "yarn spellcheck && yarn linkcheck"
  },
  "dependencies": {
    "@docusaurus/core": "3.5.2",
    "@docusaurus/preset-classic": "3.5.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-theme-redoc": "^2.0.2",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "redocusaurus": "^2.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.5.2",
    "@docusaurus/tsconfig": "3.5.2",
    "@docusaurus/types": "3.5.2",
    "cspell": "^8.14.2",
    "markdown-link-check": "^3.12.2",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "resolutions": {
    "react-router/**/path-to-regexp": "1.9.0",
    "express/**/path-to-regexp": "0.1.10"
  }
}
